{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "lambda:*"
      ],
      "Resource": [
        "arn:aws:lambda:*:*:function:awslambdaproxy"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "iam:GetRole",
        "iam:PassRole",
        "iam:PutRolePolicy",
        "iam:CreateRole"
      ],
      "Resource": [
        "arn:aws:iam::*:role/awslambdaproxy-role"
      ]
    }
  ]
}